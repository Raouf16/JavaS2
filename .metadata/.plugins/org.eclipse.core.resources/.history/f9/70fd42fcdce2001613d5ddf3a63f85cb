import java.io.IOException;
import java.lang.Math;

public class Quiz {
	int[] nombreGauche = new int[10];
	int[] nombreDroite = new int[10];
	int[] reponseUtilis = new int[10];
	
	public Quiz(){}
	
	public void createQuiz(){
		for(int i = 0 ; i < 10 ; i++){
			nombreGauche[i] = (int)(1 + (Math.random() * (100)));
		}
	}
}
